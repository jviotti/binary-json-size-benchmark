{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "additionalProperties": false,
    "required": ["optimizations"],
    "properties": {
        "optimizations": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": ["includes", "excludes", "lossy"],
                "properties": {
                    "includes": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "excludes": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "lossy": {
                        "type": "boolean"
                    }
                }
            }
        }
    }
}
